+import 'package:app/data/collections/scan_session_collection.dart';
+import 'package:app/data/collections/sensor_data_collection.dart';
+import 'package:app/models/scan_session.dart';
+import 'package:app/models/sensor_data.dart';
+import 'package:flutter/cupertino.dart';
+import 'package:isar/isar.dart';
+
+class SensorLocalDataSource {
+  final Isar isar;
+  const SensorLocalDataSource({required this.isar});
+
+  Future<bool> saveScanSession(
+    String? detectedEmotion,
+    List<SensorData> sensorDataList,
+  ) async {
+    try {
+      final scanSession = ScanSessionCollection(detectedEmotion: detectedEmotion);
+      await isar.writeTxn(() async {
+        // Save scan session first
+        final sessionId = await isar.scanSessionCollections.put(scanSession);
+
+        // Convert SensorData -> SensorDataCollection and link to session
+        final sensorEntities = sensorDataList.map((sensor) {
+          final collection = sensor.toCollection();
+          collection.scanSession.value = scanSession;
+          return collection;
+        }).toList();
+
+        // Save sensor data
+        await isar.sensorDataCollections.putAll(sensorEntities);
+
+        // Save all the links
+        for (final sensor in sensorEntities) {
+          await sensor.scanSession.save();
+        }
+      });
+      return true;
+    } catch (e) {
+      debugPrint('Error saving scan session: $e');
+      return false;
+    }
+  }
+
+  Future<List<ScanSession>> getScanSessions() async {
+    try {
+      final sessions = await isar.scanSessionCollections.where().findAll();
+      return sessions.map((session) => session.toModel()).toList();
+    } catch (e) {
+      debugPrint('Error fetching scan sessions: $e');
+      return [];
+    }
+  }
+}
